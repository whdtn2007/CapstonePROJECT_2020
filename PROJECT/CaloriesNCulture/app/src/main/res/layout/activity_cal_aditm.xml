<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainAcivity_cal_aditm">

    <TextView
        android:id="@+id/txt_calsearchplus_fooname2"
        android:layout_width="343dp"
        android:layout_height="42dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="101dp"
        android:fontFamily="@font/fontstylekor"
        android:text="음식이름"
        android:textColor="@android:color/black"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_main_calorie"
        app:layout_constraintEnd_toStartOf="@+id/btn_edit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_calsearchplus_"
        android:layout_width="84dp"
        android:layout_height="55dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="101dp"
        android:fontFamily="@font/fontstylekor"
        android:text="Kcal"
        android:textColor="@android:color/black"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_main_calorie"
        app:layout_constraintEnd_toStartOf="@+id/btn_edit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_calsearchplus_kcal2"
        android:layout_width="84dp"
        android:layout_height="55dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="101dp"
        android:fontFamily="@font/fontstylekor"
        android:text="1234"
        android:textColor="@android:color/black"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/btn_main_calorie"
        app:layout_constraintEnd_toStartOf="@+id/btn_edit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner_calsearchplus"
        android:layout_width="131dp"
        android:layout_height="61dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="20dp"
        android:entries="@array/kcal"
        app:layout_constraintBottom_toTopOf="@+id/txt_calsearchplus_3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_calsearchplus1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.501" />

    <Button
        android:id="@+id/btn_calsearchplus_save2"
        android:layout_width="119dp"
        android:layout_height="63dp"
        android:layout_marginStart="260dp"
        android:text="save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.419" />

    <EditText
        android:id="@+id/txt_calsearchplus"
        android:layout_width="214dp"
        android:layout_height="63dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="160dp"
        android:ems="10"
        android:hint="입력하세요"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/txt_calsearchplus_kcal2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_calsearchplus_fooname" />
</androidx.constraintlayout.widget.ConstraintLayout>